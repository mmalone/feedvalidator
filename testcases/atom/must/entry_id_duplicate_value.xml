<!--
  Description:  entry id values must be unique within a feed
  Expect:       DuplicateEntries{parent:entry,element:id}
-->

<feed xmlns="http://www.w3.org/2005/Atom">
<entry>
<id>http://example.com/1</id>
</entry>
<entry>
<id>http://example.com/1</id>
</entry>
</feed>

