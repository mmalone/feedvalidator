<!--
  Description:  XHTML in atom namespace
  Expect:       NotHtml{parent:content,element:content}
-->

<feed version="0.3" xmlns="http://purl.org/atom/ns#">
<entry>
<content type="multipart/alternative">
  <content type="application/xhtml+xml" xml:lang="en-us" mode="xml">  
    <p>  
       Hello, <em>weblog</em> world! 2 &lt; 4!  
    </p>  
  </content>  
</content>
</entry>
</feed>

