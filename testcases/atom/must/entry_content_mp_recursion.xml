<!--
  Description:  Multipart content can't contain multipart content
  Expect:       MultipartRecursion{parent:entry,element:content}
-->

<feed xmlns="http://www.w3.org/2005/Atom">
<entry>
<content type="multipart/alternative">
  <content type="multipart/alternative">
    <content type="text/html" xml:lang="en-us" mode="escaped">  
      <![CDATA[<p>Hello, <em>weblog</em> world! 2 &lt; 4!</p>]]>  
    </content>  
  </content>
</content>
</entry>
</feed>

