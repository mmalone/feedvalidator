<!--
  Description:  guid values in canonical form should be accepted
  Expect:       !NonCanonicalURI{}
-->

<rss version="2.0">
<channel>
<item><guid>http://example.com/</guid></item>
<item><guid>http://example.com/~smith/</guid></item>
<item><guid>http://example.com/%C3%87</guid></item>
<item><guid>http://example.com:8080/</guid></item>
<item><guid>http://example.com/?</guid></item>
<item><guid>http://example.com/#</guid></item>
<item><guid>http://example.com/?#</guid></item>
<item><guid>tag:hawke.org,2001-06-05:Taiko</guid></item>
</channel>
</rss>

