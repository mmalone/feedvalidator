<!--
  Description:  feed should not contain external http SYSTEM entities
  Expect:       ContainsSystemEntity
-->

<!DOCTYPE rss
[
<!ENTITY ping SYSTEM "http://localhost/ping">
]>
<rss version="2.0">
<channel>
<item>
<description>&lt;img src="&ping;"&gt;</description>
</item>
</channel>
</rss>

