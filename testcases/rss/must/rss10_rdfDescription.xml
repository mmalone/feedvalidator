<!--
  Description:  permit rdf:Description
  Expect:       !UndefinedElement{element:Description}
-->


<rdf:RDF
 xmlns="http://purl.org/rss/1.0/"
 xmlns:dc="http://purl.org/dc/elements/1.1/"
 xmlns:content="http://purl.org/rss/1.0/modules/content/"
 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
<channel rdf:about="http://www.example.com/">
<title>RSS 1.0 feed</title>
<description>Sample RSS 1.0 feed</description>
<link>http://www.example.com/</link>
<image rdf:resource="http://www.example.com/image" />
<dc:creator>
  <rdf:Description>
    <dc:title>me</dc:title>
  </rdf:Description>
</dc:creator>
</channel>
</rdf:RDF>

