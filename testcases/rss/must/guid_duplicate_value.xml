<!--
  Description:  guid value must be unique within a feed
  Expect:       DuplicateValue{element:guid}
-->

<rss version="2.0">
<channel>
<title>Invalid GUID</title>
<link>http://purl.org/rss/2.0/</link>
<description>foo</description>
<item>
<guid>http://example.com</guid>
</item>
<item>
<guid>http://example.com</guid>
</item>
</channel>
</rss>

