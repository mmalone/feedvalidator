<!--
  Description:  RSS 1.0 uses lowercase textinput
  Expect:       !UndefinedElement{parent:rdf:RDF,element:textinput}
-->

<rdf:RDF
 xmlns="http://purl.org/rss/1.0/"
 xmlns:dc="http://purl.org/dc/elements/1.1/"
 xmlns:content="http://purl.org/rss/1.0/modules/content/"
 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
<channel rdf:about="http://www.example.com/">
<title>RSS 1.0 feed</title>
<description>Sample RSS 1.0 feed</description>
<link>http://www.example.com/</link>
<textinput rdf:resource="http://www.example.com" />
</channel>
<textinput rdf:about="http://www.example.com/search.php">
<title>Search me</title>
<description>Search my archives</description>
<name>query</name>
<link>http://www.example.com/search.php</link>
</textinput>
</rdf:RDF>

