<!--
  Description: trackback:ping in RSS 2.0 using 1.0 syntax 
  Expect: InvalidFullLink{parent:item,element:trackback:ping}
-->

<rss version="2.0"
  xmlns:trackback="http://madskills.com/public/xml/rss/module/trackback/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
  <channel>
    <title>Trackback extension test</title>
    <link>http://example.org</link>
    <description>Foo.</description>
    <item>
      <title>Trackback in RSS 2.0</title>
      <link>http://example.org/1</link>
      <trackback:ping rdf:resource="http://example.org/tb.cgi?tb_id=1"/>
    </item>
  </channel>
</rss>
