<!--
  Description:  RSS 1.0 does not permit RSS 2.0 properties for images
  Expect:       UndefinedElement{parent:image,element:width}
-->

<rdf:RDF
 xmlns="http://purl.org/rss/1.0/"
 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
<image rdf:about='http://www.example.com/image.png'>
 <title>Title</title>
 <url>http://www.example.com/image.png</url>
 <link>http://www.example.com/</link>
 <width>100</width>
</image>
</rdf:RDF>

