<!--
  Description:  item enclosure must include non-blank integer length
  Expect:       InvalidIntegerAttribute{parent:item,element:enclosure,attr:length}
-->

<rss version="2.0">
<channel>
<title>Invalid enclosure</title>
<link>http://purl.org/rss/2.0/</link>
<description>enclosure must include integer length</description>
<item>
<title>Invalid item enclosure</title>
<link>http://purl/org/rss/2.0/?item</link>
<description>Foo</description>
<enclosure url="http://live.curry.com/mp3/celebritySCms.mp3" length="" type="audio/mpeg"/>
</item>
</channel>
</rss>

