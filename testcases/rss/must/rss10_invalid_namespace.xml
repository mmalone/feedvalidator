<!--
  Description:  Channel element is in the wrong namespace
  Expect:       MissingElement{parent:rdf:RDF,element:channel}
-->

<rdf:RDF
 xmlns="http://my.netscape.com/publish/formats/rss-0.91.dtd"
 xmlns:dc="http://purl.org/dc/elements/1.1/"
 xmlns:content="http://purl.org/rss/1.0/modules/content/"
 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
<channel rdf:about="http://www.example.com/">
<title>RSS 1.0 feed</title>
<description>Sample RSS 1.0 feed</description>
<link>http://www.example.com/</link>
<image rdf:resource="http://www.example.com/image" />
</channel>
</rdf:RDF>

