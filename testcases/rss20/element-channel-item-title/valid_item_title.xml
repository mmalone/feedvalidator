<!--
  Description:  item title must not include HTML
  Expect:       !ContainsHTML{parent:item,element:title}
-->

<rss version="2.0">
<channel>
<title>Invalid item title</title>
<link>http://purl.org/rss/2.0/</link>
<description>item title must not include HTML</description>
<item>
<title>Valid title</title>
<link>http://purl/org/rss/2.0/?item</link>
<description>Foo</description>
</item>
</channel>
</rss>

