<!--
  Description:  item enclosure must include url
  Expect:       MissingAttribute{parent:item,element:enclosure,attr:url}
-->

<rss version="2.0">
<channel>
<title>Invalid enclosure</title>
<link>http://purl.org/rss/2.0/</link>
<description>enclosure must include url attribute</description>
<item>
<title>Invalid item source</title>
<link>http://purl/org/rss/2.0/?item</link>
<description>Foo</description>
<enclosure length="1069871" type="audio/mpeg"/>
</item>
</channel>
</rss>
