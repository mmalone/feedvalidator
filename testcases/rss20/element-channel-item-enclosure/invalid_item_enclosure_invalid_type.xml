<!--
  Description:  item enclosure type must be MIME type (or at least vaguely resemble it)
  Expect:       InvalidMIMEAttribute{parent:item,element:enclosure,attr:type}
-->

<rss version="2.0">
<channel>
<title>Invalid enclosure</title>
<link>http://purl.org/rss/2.0/</link>
<description>enclosure must include type attribute</description>
<item>
<title>Invalid item source</title>
<link>http://purl/org/rss/2.0/?item</link>
<description>Foo</description>
<enclosure url="http://live.curry.com/mp3/celebritySCms.mp3" length="1069871" type="mp3"/>
</item>
</channel>
</rss>
