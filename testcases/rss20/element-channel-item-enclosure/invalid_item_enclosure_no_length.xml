<!--
  Description:  item enclosure must include length
  Expect:       MissingAttribute{parent:item,element:enclosure,attr:length}
-->

<rss version="2.0">
<channel>
<title>Invalid enclosure</title>
<link>http://purl.org/rss/2.0/</link>
<description>enclosure must include length attribute</description>
<item>
<title>Invalid item enclosure</title>
<link>http://purl/org/rss/2.0/?item</link>
<description>Foo</description>
<enclosure url="http://live.curry.com/mp3/celebritySCms.mp3" type="audio/mpeg"/>
</item>
</channel>
</rss>
