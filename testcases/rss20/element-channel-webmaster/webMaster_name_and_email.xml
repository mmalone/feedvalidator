<!--
  Description:  webMaster cannot include name and email in parentheses
  Expect:       InvalidContact{parent:channel,element:webMaster}
-->

<rss version="2.0">
<channel>
<title>Invalid webMaster</title>
<link>http://purl.org/rss/2.0/</link>
<description>webMaster must include email address</description>
<webMaster>Me (me@example.com)</webMaster>
</channel>
</rss>

