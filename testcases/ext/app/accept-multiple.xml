<!--
  Description:  Multiple accept media types on one accept element
  Expect:       !InvalidMediaRange
-->

<service xmlns="http://www.w3.org/2007/app"
         xmlns:atom="http://www.w3.org/2005/Atom">
  <workspace>
    <atom:title>Main Site</atom:title>
    <collection
        href="http://example.org/blog/pic" >
      <atom:title>Pictures</atom:title>
      <accept>image/png, image/jpeg, image/gif</accept>
    </collection>
  </workspace>
</service>
